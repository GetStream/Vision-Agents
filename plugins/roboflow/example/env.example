# Roboflow Configuration
# Get these from your Roboflow project: https://app.roboflow.com

# Your Roboflow API key (found in Settings > Roboflow API)
ROBOFLOW_API_KEY=your_api_key_here

# Your workspace ID (from project URL: /YOUR_WORKSPACE/project-name)
ROBOFLOW_WORKSPACE=your-workspace

# Your project ID (from project URL: /workspace/YOUR_PROJECT)
ROBOFLOW_PROJECT=your-project

# Model version number (default: 1)
ROBOFLOW_VERSION=1

# GetStream Configuration (required for the agent)
STREAM_API_KEY=your_stream_api_key
STREAM_API_SECRET=your_stream_api_secret

# Gemini API Key (for LLM)
GEMINI_API_KEY=your_gemini_api_key

# Cartesia API Key (for TTS)
CARTESIA_API_KEY=your_cartesia_api_key

# Deepgram API Key (for STT)
DEEPGRAM_API_KEY=your_deepgram_api_key
